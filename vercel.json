{
  "version": 2,
  "builds": [
    {
      "src": "blogpage.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "blogpage.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "functions": {
    "blogpage.py": {
      "runtime": "python3.9"
    }
  }
} 